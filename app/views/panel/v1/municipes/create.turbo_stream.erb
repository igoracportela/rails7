<%= turbo_stream.update "municipes" do %>
  <%= render partial: "panel/v1/municipes/shared/items", locals: { items: @items } %>
<% end %>

<%= turbo_stream.update "remote_modal" do %>
  <%= render partial: "panel/v1/municipes/shared/form_modal", locals: { item: @item, item_path: panel_v1_municipes_path, item_method: :post } %>
<% end %>

<% unless @item.errors.present? %>>
  <%= turbo_stream.update "modal_alert" do %>
    <%= render partial: "panel/v1/shared/alert", locals: { message: 'Enviado com sucesso', type: 'success' } %>
  <% end %>
<% end %>
